import {Recipe} from "./recipe.model"


model Submission {
  id String @id @db.Uuid @default(dbgenerated("uuid_generate_v4()"))
  imageUrl String
  approved Boolean
  userId String @db.Uuid
  recipeId String @db.Uuid
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  deletedAt DateTime?
}